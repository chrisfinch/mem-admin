{
  "defaultStacks": ["membership"],
  "packages": {
    "memb-eventbriteImages": {
      "type": "autoscaling",
      "data": {
        "bucket": "membership-dist",
        "cacheControl": "private",
        "prefixPackage": false,
        "prefixStage": false
      }
    }
  },
  "recipes": {
    "default":{
        "actionsBeforeApp": ["memb-eventbriteImages.uploadArtifacts", "memb-eventbriteImages.deploy"]
    },
    "artifactUploadOnly": {
       "actions": [ "memb-eventbriteImages.uploadArtifacts" ]
   }
  }
}
